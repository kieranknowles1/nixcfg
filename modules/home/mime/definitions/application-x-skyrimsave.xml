<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-skyrimsave">
        <comment>The Elder Scrolls V: Skyrim Save File</comment>
        <glob pattern="*.ess"/>
        <magic priority="50">
            <match value="TESV_SAVEGAME" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
